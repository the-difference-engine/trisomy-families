    </div>
    <div>
      <%= label_tag :parent_1_state %>
      <%= select_tag(:parent_1_state, options_for_select(us_states, "IL")) %>
    </div>
    <div>
      <%= label_tag :parent_1_city %>
      <%= text_field_tag :parent_1_city %>
    </div>
    <div>
      <%= label_tag :parent_1_email %>
      <%= text_field_tag :parent_1_email %>
    </div>
  </div>
  <div>
